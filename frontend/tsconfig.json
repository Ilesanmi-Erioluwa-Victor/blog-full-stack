{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "."
  },
  "include": [
    "react-app-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/header/header.jsx",
    "src/redux/Slices/users/user.tsx",
    "src/pages/auth/Signup.js",
    "src/pages/auth/Login.tsx",
    "src/App.tsx",
    "src/index.tsx",
    "src/pages/Admin/Addcategory/Addcategory.js",
    "src/pages/Admin/Category/Category.js",
    "src/pages/Admin/Admin.js",
    "src/pages/Home.tsx",
    "src/redux/store.ts",
    "src/components/molecules/Navigation/Private/PrivateNav.tsx",
    "src/components/molecules/Footer/Footer.js",
    "src/components/molecules/Navigation/Navigation.tsx",
    "src/components/molecules/Navigation/Public/PublicNav.tsx",
    "src/pages/Admin/index.js",
    "src/components/atoms/Dropdown/Dropdown.js",
    "src/components/atoms/Loader.jsx",
    "src/pages/Admin/Post/Post.jsx"
  ],
  "exclude": ["node_modules"]
}
